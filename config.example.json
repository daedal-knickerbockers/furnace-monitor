{
    "logLevel": "info",
    "localConfigDirPath": "/var/lib/furnace-monitor",
    "gpioBasePath": "/sys/class/gpio",
    "consumers": {
        "Furnace": {
            "gpio": 1
        },
        "Heatpump": {
            "gpio": 2
        }
    },
    "database": {
        "path": "/var/lib/furnace-monitor/database.sqlite",
        "fileExport": {
            "exportDirectory": "/var/lib/furnace-monitor/export",
            "interval": "DAILY"
        }
    },
    "resolSensors": {
        "myCoolSensor": {
            "host": "192.168.178.123",
            "password": "fooBarPassword"
        }
    }
}
